<launch>
  <include file="$(find pr2_machine)/$(env ROBOT).machine" />
  <include file="$(find pr2_navigation_slam)/slam_gmapping.xml" />
  <include file="$(find pr2_navigation_perception)/lasers_and_filters.xml" />
  <include file="$(find pr2_navigation_perception)/ground_plane.xml" />
  <include file="$(find pr2_navigation_slam)/move_base.xml" />
  <include file="$(find explore_pr2)/explore.xml" />  
  <!--include file="$(find pr2_navigation_teleop)/teleop.xml" /-->  
  <!-- Bring up a relay to forward the navigation commands. This lets use reuse move_base.xml from pr2_navigation_slam directly.-->
  <node pkg="topic_tools" type="relay" name="cmd_vel_relay" respawn="true" args="/navigation/cmd_vel /base_controller/command"/>
</launch>
